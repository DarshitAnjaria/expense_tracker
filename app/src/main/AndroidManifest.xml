<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.expensetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_ie1"
        android:label="Expense Tracker"
        android:roundIcon="@mipmap/ic_launcher_ie1"
        android:supportsRtl="true"
        android:theme="@style/MyTheme">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".StartActivity" />
        <activity
            android:name=".VerifyPhoneActivity"
            android:parentActivityName=".StartActivity" />
        <activity
            android:name=".AddingActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ReportActivity" />
        <activity android:name=".SmartSuggestionActivity" />
        <activity android:name=".TaxCalculator"></activity>
    </application>

</manifest>